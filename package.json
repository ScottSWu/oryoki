{
  "name": "oryoki",
  "version": "0.0.5-1",
  "productName": "Oryoki",
  "description": "A web browser with a thin interface.",
  "repository": {
    "type": "git",
    "url": "https://github.com/thmsbfft/oryoki.git"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "gulp build && electron-packager . Oryoki --overwrite --out=build --app-version=0.0.5 --platform=win32 --arch=x64 --icon=icon.icns --ignore=\"(src/sass)|(src/js/main)|build|gulpfile|doc|stdout.log\""
  },
