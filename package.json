    "png-chunks-encode": "^1.0.0",
    "png-chunks-extract": "^1.0.0",
    "preprocess": "^3.0.2",
    "request": "^2.75.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "electron-packager": "^8.0.0",
    "electron-prebuilt": "^1.4.1",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-copy": "0.0.2",
    "gulp-exec": "^2.1.2",
    "gulp-livereload": "^3.8.1",
    "gulp-preprocess": "^2.0.0",
